local Anim = Instance.new("Animation")
				Anim.AnimationId = "http://www.roblox.com/asset/?id=3337994105"
				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed(1.8)
			wait(1.6)
					CurrAnim:Stop()
